# Binance API Configuration
# Get your testnet keys from: https://testnet.binance.vision/

# Set to true for testnet, false for production
BINANCE_USE_TESTNET=true

# Testnet API Keys (safe to test)
BINANCE_API_KEY=your_testnet_api_key_here
BINANCE_SECRET_KEY=your_testnet_secret_key_here

# Trading Pairs
DEFAULT_SYMBOL=BTCUSDT
DEFAULT_SYMBOLS=BTCUSDT,ETHUSDT,BNBUSDT,SOLUSDT,ADAUSDT,XRPUSDT,DOTUSDT,DOGEUSDT,MATICUSDT

# WebSocket Settings
WS_RECONNECT_DELAY=5
